		<schema:sameAs rdf:datatype="https://schema.org/URL">https://archives.seoul.go.kr/item/15</schema:sameAs>
		<schema:identifier>RG7-SR8-FI16-IT15</schema:identifier>
		<schema:name>USAFFE Interrogation Report No. 131</schema:name>
	</rdf:Description>

